{
  "manifest_version": 3,
  "name": "InMailX",
  "version": "0.1.0",
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "content.js"
      ],
      "matches": [
        "https://www.linkedin.com/*"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": { "48": "icon48.png" },
  "permissions": ["storage", "tabs", "activeTab"]
}
